<%= table_for @scientific_ranks do |t| %>
    <%= t.body do |scientific_rank| %>
        <%= t.r do %>
            <%= t.d link_to(scientific_rank.name, { :controller => '/admin/dictionary/scientific_rank', :action => 'edit', :id => scientific_rank}, {:method => 'get'}) %>
            <%= t.d button_to("изменить", { :controller => '/admin/dictionary/scientific_rank', :action => 'edit', :id => scientific_rank}, {:method => 'get', :class => 'edit'}) %>
            <%= t.d button_to("удалить", { :controller => '/admin/dictionary/scientific_rank', :action => 'destroy', :id => scientific_rank}, {:class => 'destroy'}) %>
        <% end %>
    <% end %>
<% end %>
