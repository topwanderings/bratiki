<%= table_for @groups do |t| %>
    <%= t.body do |group| %>
        <%= t.r do %>
            <%= t.d link_to(group.number, { :controller => '/teacher/institute/chair/group', :action => 'show', :group_id => group, :institute_id => params[:institute_id], :chair_id => params[:chair_id] }, {:method => 'get'}) %>
        <% end %>
    <% end %>
<% end %>


